{
  "name": "railstats_api",
  "version": "0.1.0",
  "description": "Performance data API for railstats.org",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec babel-node src",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "test": "mocha --require @babel/register test/unit test/integration"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "metro-ontime/railstats_api",
  "author": "Cameron Sexton",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.417.0",
    "body-parser": "^1.20.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^0.6.1",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "eslint": "^3.1.1",
    "mocha": "^6.0.2",
    "nodemon": "^1.9.2",
    "supertest": "^4.0.0"
  }
}
